{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "max-classes-per-file": false,
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "no-alias-methods": true
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["./counter/__specs__"]
    }
}